{
  "name": "noders.services",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build-staging": "yarn prebuild && BASE_URL=/service/ docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prebuild": "tsc --project tsconfig.client.json && node scripts/convert-icons.js"
  },
  "dependencies": {
    "@acid-info/docusaurus-og": "^1.0.2",
    "@docusaurus/core": "^3.8.0",
    "@docusaurus/faster": "^3.8.0",
    "@docusaurus/preset-classic": "^3.8.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inconsolata": "^5.2.5",
    "@fontsource/space-grotesk": "^5.2.6",
    "@mdx-js/react": "^3.1.0",
    "@mui/material": "^7.0.2",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.8.0",
    "@docusaurus/tsconfig": "^3.8.0",
    "@docusaurus/types": "^3.8.0",
    "glob": "^11.0.2",
    "prettier": "^3.5.3",
    "sharp": "^0.34.1",
    "typescript": "^5.8.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=22.0"
  },
  "packageManager": "yarn@1.22.22"
}
